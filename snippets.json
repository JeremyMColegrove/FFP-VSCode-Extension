{
    "If Statement": {
        "prefix": "if",
        "body": [
            "IF $1 THEN",
            "\t$2",
            "ENDIF"
        ],
        "description": "If statement"
    },
    "Classic For Loop": {
        "prefix": "for",
        "body": [
            "FOR $1 FROM $2 TO $3 DO",
            "\t$4",
            "ENDFOR"
        ],
        "description": "For loop"
    },
    "Iterate Over Array": {
        "prefix": "for",
        "body": [
            "FOR $1 IN $2 DO",
            "\t$3",
            "ENDFOR"
        ],
        "description": "For loop"
    },
    "Split String to Array": {
        "prefix": "split",
        "body": [
            "SPLIT $1 BY $2 TO $3"
        ]
    },
    "Regex Match Boolean": {
        "prefix": "matches",
        "body": [
            "\"$1\" MATCHES /$2/"
        ]
    },
    "Set Variable": {
        "prefix": "set",
        "body": [
            "SET $1 TO $2"
        ]
    },
    "Read from File": {
        "prefix": "read",
        "body": [
            "READ $1 TO $2"
        ]
    },
    "Write to File": {
        "prefix": "write",
        "body": [
            "WRITE $1 TO $2"
        ]
    },
    "Append to File/Array": {
        "prefix": "append",
        "body": [
            "APPEND $1 TO $2"
        ]
    },
    "Substring of Array/String": {
        "prefix": "substring",
        "body": [
            "SUBSTRING $1 FROM $2 TO $3"
        ]
    },
    "Print Value": {
        "prefix": "print",
        "body": [
            "PRINT $1"
        ]
    },
    "Reverse Array/String": {
        "prefix": "reverse",
        "body": [
            "REVERSE $1"
        ]
    },
    "Trim String": {
        "prefix": "trim",
        "body":[
            "TRIM $1"
        ]
    },
    "Sort Array/String": {
        "prefix": "sort",
        "body": [
            "SORT $1"
        ]
    },
    "Length of Array/String": {
        "prefix": "length",
        "body":[
            "LENGTH $1"
        ]
    },
    "Auto Add Newline Directive": {
        "prefix": "@autonewline",
        "body":["@AUTONEWLINE"]
    },
    "Sleep": {
        "prefix": "sleep",
        "body":["SLEEP $1"]
    },
    "Find Regex Matches in Array": {
        "prefix":"find",
        "body":["FIND /$1/ IN $2"]
    },
    "Replace All Matches": {
        "prefix":"replace",
        "body":["REPLACE $1 WITH $2 IN $3"]
    },
    "Replace First Match": {
        "prefix":"replace",
        "body":["REPLACE FIRST $1 WITH $2 IN $3"]
    },
    "Copy File": {
        "prefix":"copy",
        "body":["COPY $1 TO $2"]
    },
    "Delete File": {
        "prefix":"delete",
        "body":["DELETE $1"]
    },
    "Join Array with Delimiter": {
        "prefix":"join",
        "body":["JOIN $1 WITH $2"]
    },
    "Assert Condition": {
        "prefix":"assert",
        "body":["ASSERT $1"]
    },
    "Send Network Request": {
        "prefix":"send",
        "body":["SEND $1 REQUEST TO $2 WITH BODY \"$3\" WITH HEADERS \"$4\" TO $5"]
    }
}